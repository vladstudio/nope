{
  "manifest_version": 3,
  "name": "Nope",
  "version": "1.0",
  "description": "Block distracting websites with style",
  "permissions": ["storage", "webNavigation"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon/nope-off.png"
  },
  "icons": {
    "128": "icon/nope-on.png"
  }
}
